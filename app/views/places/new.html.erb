<div class="booyah-box text-center">
	<h1>Share Your Fav! Place Here</h1><br />
	<%= simple_form_for @places do |f| %>
		<%= f.input :name, :class => "col-center" %><br />
		<%= f.input :address %><br />
		<%= f.input :description %><br /><br />
		<%= f.submit 'Create', :class => "btn btn-primary" %>
	<% end %>
</div>